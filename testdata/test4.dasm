;All operand types, presented in various ways

:start	set a, 5
		set a, b
		set a, [b]
		set a, [b + 3]
		set a, [3 + b]	;both ways should be acceptable
		set a, [b+3]	;spaces should not be forced
		set a, POP
		set a, PEEK
		set PUSH, a
		set PICK 3, a
		set a, start
		set a, [start]
		set a, PC
		set a, SP
		set a, EX
		
		;reverse these
		
		;set 5, a		;allowable in the spec though
		
		set b, a
		set [b], a
		set [b + 3], a
		set [3 + b], a
		set [b+3], a
		set a, PICK 4
		set start, a
		set [start], a
		set PC, a
		set SP, a
		set EX, a

